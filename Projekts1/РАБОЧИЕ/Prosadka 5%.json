      "parameters": {
        "jsCode": "const out = [];\n\nfor (const item of items) {\n  try {\n    const securities = item.json.securities;\n    if (!securities || !securities.data || !securities.data.length) continue;\n\n    const data = securities.data[0];\n    const columns = securities.columns;\n\n    const secidIndex = columns.indexOf(\"secid\");\n    const boardIndex = columns.indexOf(\"marketprice_boardid\");\n\n    const secid = data[secidIndex];\n    const boardid = data[boardIndex];\n\n    out.push({\n      json: {\n        SECID: secid,\n        BOARDID: boardid\n      }\n    });\n  } catch (e) {\n    // –µ—Å–ª–∏ –¥–∞–Ω–Ω—ã—Ö –Ω–µ—Ç ‚Äî –ø—Ä–æ–ø—É—Å–∫–∞–µ–º\n  }\n}\n\nreturn out;\n"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
